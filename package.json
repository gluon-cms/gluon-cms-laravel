{
    "private": true,
    "scripts": {
        "front-dev": "npm run front-development",
        "front-watch": "npm run front-development -- --watch",
        "front-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=webpack.mix.front.js",
        "front-prod": "npm run front-production",
        "front-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=webpack.mix.front.js",
        "back-dev": "npm run back-development",
        "back-watch": "npm run back-development -- --watch",
        "back-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=webpack.mix.back.js",
        "back-prod": "npm run back-production",
        "back-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=webpack.mix.back.js",
        "build": "npm run front-prod && npm run back-prod"
    },
    "devDependencies": {
